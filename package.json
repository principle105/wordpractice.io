{
    "name": "wordpractice.io",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "dev": "vite dev",
        "build": "prisma generate && vite build",
        "start": "tsm ./server/index.ts",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "prettier --plugin-search-dir . --check . && eslint .",
        "lint:fix": "eslint --fix src && prettier --plugin prettier-plugin-svelte --write .",
        "format": "prettier --plugin-search-dir . --write ."
    },
    "devDependencies": {
        "@iconify-json/bi": "^1.1.23",
        "@iconify-json/fa6-regular": "^1.1.18",
        "@iconify-json/ic": "^1.1.17",
        "@iconify-json/material-symbols": "^1.1.76",
        "@iconify-json/mdi": "^1.1.64",
        "@iconify-json/nimbus": "^1.1.11",
        "@sveltejs/adapter-auto": "^2.1.1",
        "@sveltejs/adapter-node": "^1.3.1",
        "@sveltejs/kit": "^1.30.4",
        "@types/express": "^4.17.21",
        "@types/node": "^20.11.30",
        "@typescript-eslint/eslint-plugin": "^5.62.0",
        "@typescript-eslint/parser": "^5.62.0",
        "autoprefixer": "^10.4.19",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^8.10.0",
        "eslint-plugin-svelte": "^2.35.1",
        "postcss": "^8.4.38",
        "prettier": "^2.8.8",
        "prettier-plugin-svelte": "^2.10.1",
        "svelte": "^4.2.12",
        "svelte-check": "^3.6.8",
        "svelte-french-toast": "^1.2.0",
        "sveltekit-flash-message": "^2.4.4",
        "tailwindcss": "^3.4.1",
        "tslib": "^2.6.2",
        "tsm": "^2.3.0",
        "typescript": "^5.4.3",
        "unplugin-icons": "^0.18.5",
        "vite": "^4.5.3"
    },
    "type": "module",
    "dependencies": {
        "@lucia-auth/adapter-prisma": "^4.0.1",
        "arctic": "^1.2.1",
        "express": "^4.19.2",
        "lucia": "^3.1.1",
        "oslo": "^1.1.3",
        "prisma": "^5.11.0",
        "socket.io": "^4.7.5",
        "socket.io-client": "^4.7.5"
    }
}
